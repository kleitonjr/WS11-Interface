package model.services;

import model.entities.Invoice;
import model.entities.Rent;

public class RentServices{
	
	private Double perDay;
	private Double perHour;
	
	private BrazilTaxServices taxService;

	public RentServices(Double perDay, Double perHour, BrazilTaxServices taxService) {
		this.perDay = perDay;
		this.perHour = perHour;
		this.taxService = taxService;
	}
	
	public void processInvoice(Rent rent) {
		rent.setInvoice(new Invoice(50.0, 10.0));
		
	}


}
